{
  "name": "@saas-platform/landing",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3000",
    "build": "next build --no-lint",
    "start": "next start",
    "postinstall": "prisma generate --schema=./prisma/schema.prisma && npm run copy-prisma-engines",
    "copy-prisma-engines": "mkdir -p .prisma/client && cp -r node_modules/.prisma/client/* .prisma/client/ 2>/dev/null || true",
    "lint": "next lint",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@prisma/client": "5.22.0",
    "@saas-platform/auth": "workspace:*",
    "@saas-platform/database": "workspace:*",
    "@saas-platform/types": "workspace:*",
    "@saas-platform/ui": "workspace:*",
    "@saas-platform/utils": "workspace:*",
    "bcryptjs": "^2.4.3",
    "framer-motion": "^12.23.24",
    "lucide-react": "^0.303.0",
    "next": "^14.0.4",
    "next-auth": "^4.24.5",
    "next-themes": "^0.4.6",
    "prisma": "5.22.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sonner": "^1.3.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^20.10.6",
    "@types/react": "^18.2.46",
    "@types/react-dom": "^18.2.18",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.56.0",
    "eslint-config-next": "^14.0.4",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.3.3"
  }
}
